{
  "name": "Proofy",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Hack ZÃ¼rich 2019",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
